{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "extends": [
    // "plugin:@typescript-eslint/recommended",
    "eslint:recommended"
  ],
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "plugins": [
    // "@typescript-eslint"
  ],
  "rules": {
    "no-unused-vars": "off",
    "no-unsafe-argument": "off",
    "no-unsafe-call": "off",
    "no-unsafe-assignment": "off"
  },
  "globals": {
    "navigator": false,
    "document": false,
    "unescape": false,
    "describe": true,
    "escape": false,
    "window": false,
    "before": true,
    "expect": true,
    "THREE": false,
    "sinon": true,
    "it": true
  },
  "env": {
    "browser": true,
    "node": true,
    "es6": true
  }
}
